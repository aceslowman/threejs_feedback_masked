{
  "name": "threejs_node",
  "version": "1.0.0",
  "description": "modular THREEjs boilerplate, built off of Node, Express, and Webpack",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --config webpack.dev.config.js --mode development --watch",
    "build": "webpack --config webpack.prod.config.js --mode production --optimize-minimize --watch",
    "start": "nodemon server.js --watch server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aceslowman/threejs_node.git"
  },
